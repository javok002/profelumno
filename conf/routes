# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET        /                       ua.dirproy.profelumno.controllers.Application.index()

->         /user                   user.Routes
->         /teacher-profile        teacherProfile.Routes
->         /register               register.Routes
->         /delete                 delete.Routes
->         /modify-teacher         teacherModification.Routes
->         /teacher-search         teacherSearch.Routes
->         /mail-sender            mailSender.Routes
->         /modify-student         studentModification.Routes



# Map static resources from the /public folder to the /assets URL path
GET        /assets/*file           controllers.Assets.versioned(path="/public", file: Asset)
